{
  "presets": [
      "env"
  ],
  "plugins": [
      [
          "transform-runtime", // 'transform-runtime' 插件告诉 babel 要引用 runtime 来代替注入。
          {
              "helpers": true,
              "polyfill": true,
              "regenerator": true,
              "moduleName": "babel-runtime"
          }
      ]
  ]
}